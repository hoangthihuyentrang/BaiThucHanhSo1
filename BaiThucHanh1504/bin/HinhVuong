package shapes;

import java.util.Scanner;
public class HinhVuong extends HinhchuNhat{
    private Object dài;
    private float rong;
    private String Ten;
    //constructor
    public HinhVuong(){
        Ten = "Hinh vuong";
    }
    public void nhapCanh(){
        System.out.println("canh: ");
        Scanner sc=new Scanner(System.in);
        dài = rong =sc.nextFloat();
    }
}
